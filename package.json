{
  "name": "ngx-propagation-stop",
  "version": "1.0.0",
  "description": "Stop Click Propagation on any Element.",
  "keywords": ["angular", "propagation"],
  "bugs": {
    "url": "https://github.com/DanielHabenicht/ngx-propagation-stop/issues"
  },
  "author": {
    "name": "Daniel Habenicht",
    "email": "daniel-habenicht@aquiver.de",
    "url": "https://www.aquiver.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielHabenicht/ngx-propagation-stop"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "build-prod": "ng build --prod --build-optimizer --aot",
    "test": "ng test --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "pack": "npm pack ./dist",
    "release": "cd dist && semantic-release",
    "commit": "git add * && git-cz"
  },
  "private": false,
  "dependencies": {
    "@angular/core": "^7.0.0"
  },
  "devDependencies": {
    "@angular/cli": "7.0.2",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.12.0",
    "codelyzer": "^4.0.1",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.1",
    "semantic-release": "^15.0.2",
    "travis-deploy-once": "^5.0.9",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "zone.js": "^0.8.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": ["jasmine-core"]
  }
}
