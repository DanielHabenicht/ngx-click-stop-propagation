{
  "name": "ngx-propagation-stop-app",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "build-prod": "ng build --prod --build-optimizer --aot",
    "test": "ng test ngx-propagation-stop-app",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --project ngx-propagation-stop && npm run copy-files",
    "copy-license": "cp ./LICENSE ./dist/ngx-propagation-stop",
    "copy-readme": "cp ./README.md ./dist/ngx-propagation-stop",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "commit": "git add * && git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "rxjs": "6.5.4",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.25",
    "@angular-devkit/build-ng-packagr": "0.803.25",
    "@angular/cli": "8.3.25",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.5.7",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.28",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "ng-packagr": "5.7.1",
    "protractor": "5.4.3",
    "puppeteer": "2.1.1",
    "semantic-release": "15.14.0",
    "ts-node": "8.6.2",
    "tsickle": "0.38.0",
    "tslib": "1.11.1",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "webdriver-manager": "12.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "pkgRoot": "dist/ngx-propagation-stop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielHabenicht/ngx-propagation-stop.git"
  }
}
