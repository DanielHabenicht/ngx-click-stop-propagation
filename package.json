{
  "name": "ngx-propagation-stop-app",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "build-prod": "ng build --prod --build-optimizer --aot",
    "test": "ng test ngx-propagation-stop-app",
    "lint": "ng lint",
    "e2e": "webdriver-manager update --versions.chrome=80.0.3987.106 --gecko false --standalone false && ng e2e --webdriverUpdate false",
    "build:lib": "ng build --project ngx-propagation-stop && npm run copy-files",
    "copy-license": "cp ./LICENSE ./dist/ngx-propagation-stop",
    "copy-readme": "cp ./README.md ./dist/ngx-propagation-stop",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "commit": "git add * && git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.7",
    "@angular/common": "9.1.7",
    "@angular/compiler": "9.1.7",
    "@angular/core": "9.1.7",
    "@angular/forms": "9.1.7",
    "@angular/platform-browser": "9.1.7",
    "@angular/platform-browser-dynamic": "9.1.7",
    "@angular/router": "9.1.7",
    "rxjs": "6.5.5",
    "tslib": "1.13.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.6",
    "@angular-devkit/build-ng-packagr": "0.901.6",
    "@angular/cli": "9.1.6",
    "@angular/compiler-cli": "9.1.7",
    "@angular/language-service": "9.1.7",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.39",
    "codelyzer": "5.2.2",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "9.1.5",
    "protractor": "5.4.4",
    "puppeteer": "2.1.1",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "webdriver-manager": "12.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "pkgRoot": "dist/ngx-propagation-stop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielHabenicht/ngx-propagation-stop.git"
  }
}
